<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.homechart.app.smartrefreshlayout">

    <application
        android:name=".MyApplication"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".type.OneActivity"/>
        <activity android:name=".type.TwoActivity"/>
        <activity android:name=".type.ThreeActivity"/>
        <activity android:name=".type.FourActivity"/>
        <activity android:name=".type.FiveActivity"/>
        <activity android:name=".type.SixActivity"/>
        <activity android:name=".type.SevenActivity"/>
        <activity android:name=".type.EightActivity"/>
        <activity android:name=".type.NineActivity"/>
        <activity android:name=".type.TenActivity"/>
    </application>

</manifest>